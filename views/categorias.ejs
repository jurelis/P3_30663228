<!DOCTYPE html>
<html lang="es">
<head>
  <meta charset="UTF-8">
  <title>Fit_Life-categoriass</title>
  <link rel="stylesheet" type="text/css" href="/css/producto.css">
  <link rel="icon" type="image/x-icon" href="img/Dental.jpg" />
</head>
<body>
  <h1 style="color: rgb(235, 120, 216);">Categorias</h1>
  <div class="table">
    <table border>
          <tr>
            <th>ID</th>
            <th>Nombre</th>
            <th><a href="/addCategorias"><img src="/img/add.png" id='add'></a></th>
          </tr>
          <%for(const categorias of modelo){%>
            <tr>
              <td><%=categorias.id%></td>
              <td><%=categorias.nombre%></td>
              <td class="icon">

              <a href="/updateCategoria/<%=categorias.id%>"><img src="/img/update.png" width="30"></a>

             </td>
            </tr>
           <%}%>
        </table>
       
  </div>
  <button class="intel">
  <a id='botonProductos' href="/productos" style="color: white;">Volver</a>
</button>
</body>
</html>
